{
  "name": "dynamic-styleguide",
  "version": "1.2.2",
  "description": "Tool to dynamically generate styleguides from UI library. Used either internaly while working on components/modules or to share documentation with clients.",
  "engines": {
    "node": ">=8.9.0"
  },
  "main": "index.js",
  "license": "MIT",
  "private": false,
  "scripts": {
    "postinstall": "yarn build",
    "start": "node index.js",
    "dev": "yarn dev:ui & yarn dev:server",
    "dev:ui": "webpack --mode development --watch --config ./build/webpack.config.js",
    "build": "webpack --mode production --config ./build/webpack.config.js",
    "dev:server": "NODE_ENV=development browser-refresh index.js",
    "test:unit": "NODE_ENV=test mocha --timeout 5000 './test/**/*-test.js'",
    "test:lint": "eslint src/**/*.js",
    "test": "yarn test:lint && yarn test:unit"
  },
  "bin": {
    "dynamic-styleguide": "./bin/runner.js"
  },
  "keywords": [
    "dynamic",
    "style",
    "guide",
    "docs",
    "documentation",
    "styleguide",
    "hanzo",
    "component",
    "front end",
    "SCSS",
    "CSS"
  ],
  "author": "Hanzo SL",
  "dependencies": {
    "autoprefixer": "^8.5.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "browser-refresh": "^1.7.2",
    "clean-webpack-plugin": "^0.1.19",
    "codemirror": "^5.36.0",
    "cookie-parser": "~1.4.3",
    "copy-webpack-plugin": "^4.5.1",
    "css-hot-loader": "^1.3.9",
    "css-loader": "^0.28.11",
    "debug": "~2.6.9",
    "deep-assign": "^2.0.0",
    "express": "~4.16.0",
    "file-loader": "^1.1.11",
    "hbs": "~4.0.1",
    "html-entities": "^1.2.1",
    "json-loader": "^0.5.7",
    "mamacro": "^0.0.3",
    "modernizr": "^3.6.0",
    "modernizr-loader": "^1.0.1",
    "morgan": "~1.9.0",
    "nanoevents": "^1.0.5",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.3",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^6.0.7",
    "sass-thematic": "^2.0.4",
    "sassdoc": "^2.5.0",
    "shelljs": "^0.8.1",
    "showdown": "^1.8.6",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.8.3",
    "xml2json": "^0.11.2",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^5.1.1",
    "supertest": "^3.0.0",
    "webpack-cli": "^2.1.3",
    "write-file-webpack-plugin": "^4.3.2"
  }
}
